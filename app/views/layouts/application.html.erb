<!DOCTYPE html>
<html>
<head>
  <title>Placero</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/Swiper/3.3.0/css/swiper.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/Swiper/3.3.0/css/swiper.min.css">
   
  <script src="https://cdnjs.cloudflare.com/ajax/libs/Swiper/3.3.0/js/swiper.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/Swiper/3.3.0/js/swiper.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/Swiper/3.3.0/js/swiper.jquery.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/Swiper/3.3.0/js/swiper.jquery.min.js"></script>
</head>
<body>
<% if user_signed_in? %>
  <div>
    Signed in as: <%= current_user.email %>
    <%= button_to 'Sign out', destroy_user_session_path, method: :delete %>
    <%= link_to "post new place", places_new_path %>
  </div>
<% else %>

  <%= link_to "Continue with Facebook", user_omniauth_authorize_path(:facebook),id: "login_facebook" %>

<% end %>
<%= yield %>
	 
</body>
</html>
