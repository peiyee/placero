<div class="top_bar">

<a href="/"><div id="create_nav"><img class="arrow" src="/assets/backarrow.png"> Create New Place</div></a>

</div>

<div class="center_form">
<%= form_for @place, url: places_path do |f| %>
    
<div class="float_left_form">
   
	<div>
		<%= f.label "Name" %><br>
		<%= f.text_field :name %>
	</div>
	<div>
		<%= f.label "Email" %><br>
		<%= f.email_field :email %>
	</div>
	<div>
		<%= f.label "Address line" %><br>
		<%= f.text_field :line %>
	</div>
	<div>
		<%= f.label "Address line 2" %><br>
		<%= f.text_field :line2 %>
	</div>
</div>

<div class="float_right_form">
	<div>
		<%= f.label "Postcode" %><br>
		<%= f.text_field :postcode %>
	</div>
	<div>
		<%= f.label "City" %><br>
		<%= f.text_field :city %>
	</div>
	<div>
		<%= f.label "Country" %><br>
		<%= f.text_field :country %>
	</div>
	<div>
		<%= f.file_field :avatars, multiple: true %>
	</div>	
	<div>
		<%= f.label :tag_list, "Tags (seperated by commas)" %><br>
		<%= f.text_field :tag_list %>
	</div>
	<div>
		<%= f.submit %>
	</div>
</div>
<% end %>

